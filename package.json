{
  "name": "vscode-mezzurite",
  "description": "Instrument your application with the Mezzurite SDK quickly and easily with this extension.",
  "displayName": "Mezzurite",
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "publisher": "mezzurite-devs",
  "categories": [
    "Linters",
    "Snippets"
  ],
  "keywords": [
    "Microsoft",
    "Angular",
    "React",
    "single page application"
  ],
  "engines": {
    "vscode": "1.31.1"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./client/out/extension",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "mezzuriteComponentList",
          "name": "Mezzurite Component Tracking"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc -b",
    "lint": "tslint -p ./tslint.json",
    "postinstall": "cd client && npm install && cd ../server && npm install && cd ..",
    "test": "jest",
    "vscode:prepublish": "cd client && npm run update-vscode && cd .. && npm run compile",
    "watch": "tsc -b -w",
    "prepublishOnly": "TypeScriptESBuild Source/**/*.ts"
  },
  "devDependencies": {
    "@types/jest": "29.5.6",
    "@types/node": "20.8.9",
    "jest": "29.7.0",
    "ts-jest": "29.1.1",
    "tslint": "5.20.1",
    "tslint-config-semistandard": "8.0.1",
    "typescript": "5.2.2",
    "typescript-esbuild": "0.3.1"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "type": "module",
  "private": "false",
  "publishConfig": {
    "access": "public"
  }
}
